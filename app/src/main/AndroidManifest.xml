<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhijeetsingh.bizopt">
    <uses-feature android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Shanaya"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Sign_in" />
        <activity android:name=".Sign_up" />
        <activity android:name=".Trainee"
            android:label="Trainee"/>
        <activity android:name=".Employee"
            android:label="Employee"/>
        <activity android:name=".HR"
            android:label="HR"/>
        <activity android:name=".Vendor"
            android:label="Vendor">

        </activity>
    </application>

</manifest>